<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data>
        <record id="stock_inventory_wizard_report_view" model="ir.ui.view">
            <field name="name">Stock in Date</field>
            <field name="model">stock.inventory.wzd</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <form string="Stock in Date">
                <field name="date"/>
                <field name="company_id" widget="selection"/>
                <field name="location_id"/>
                <separator colspan="4"/>
                <group col="4" colspan="4">
                    <button icon="gtk-cancel" special="cancel" string="Cancel" colspan="2"/>
                    <button name="print_report" string="_Print" type="object" icon="gtk-print" default_focus="1"/>
                </group>
            </form>
            </field>
         </record>

    </data>
</openerp>
